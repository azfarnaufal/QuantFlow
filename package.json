{
  "name": "quantflow",
  "version": "1.0.0",
  "description": "Quantitative trading platform with real-time market data, ML predictions, and backtesting engine",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "node server.js",
    "server:tsdb": "STORAGE_TYPE=timescaledb node server-timescaledb.js",
    "alerts": "node src/core/price-alert-system.js",
    "dashboard": "node dashboard.js",
    "nodered": "node src/core/nodered-integration.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "quantitative-finance",
    "trading",
    "machine-learning",
    "backtesting",
    "binance",
    "cryptocurrency",
    "websocket"
  ],
  "author": "Azfar Naufal",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ]
  }
}
